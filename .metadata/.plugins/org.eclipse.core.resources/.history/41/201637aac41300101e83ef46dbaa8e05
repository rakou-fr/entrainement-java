
abstract public class VéhiculeTransportMarchandises extends Véhicule{
	private float chargeMax;
	private float chargeUtile;
	
	public VéhiculeTransportMarchandises(float chargeMax, float chargeUtile) {
		this.chargeMax = chargeMax;
		this.chargeUtile = chargeUtile;
	}
	
	public float getChargeMax() {
		return this.chargeMax;
	}
	
	public float getChargeUtile() {
		return this.chargeUtile;
	}
}
